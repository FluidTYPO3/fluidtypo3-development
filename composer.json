{
    "name": "fluidtypo3/development",
    "description": "FluidTYPO3 development helpers - test runners, coding standards sniffing, local git hook scripts",
    "license": "LGPL-3.0-or-later",
    "keywords": ["php", "git", "fluidtypo3", "typo3", "helper"],
    "authors": [
        {
            "name": "Claus Due",
            "email": "claus@namelesscoder.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "FluidTYPO3\\Development\\": "src/"
        }
    },
    "require": {
        "typo3/cms-core": ">=7.6.13, <7.6.99|^8|^9|dev-master",
        "typo3/cms-info": ">=7.6.13, <7.6.99|^8|^9|dev-master",
        "typo3/cms-beuser": ">=7.6.13, <7.6.99|^8|^9|dev-master",
        "typo3/cms-extbase": ">=7.6.13, <7.6.99|^8|^9|dev-master",
        "typo3/cms-fluid": ">=7.6.13, <7.6.99|^8|^9|dev-master",
        "typo3/cms-backend": ">=7.6.13, <7.6.99|^8|^9|dev-master",
        "typo3/cms-install": ">=7.6.13, <7.6.99|^8|^9|dev-master",
        "typo3/cms-setup": ">=7.6.13, <7.6.99|^8|^9|dev-master",
        "typo3/cms-filelist": ">=7.6.13, <7.6.99|^8|^9|dev-master",
        "typo3/cms-recordlist": ">=7.6.13, <7.6.99|^8|^9|dev-master",
        "typo3/cms-scheduler": ">=7.6.13, <7.6.99|^8|^9|dev-master",
        "typo3/cms-tstemplate": ">=7.6.13, <7.6.99|^8|^9|dev-master",
        "typo3/cms-workspaces": ">=7.6.13, <7.6.99|^8|^9|dev-master",
        "typo3/cms-taskcenter": ">=7.6.13, <7.6.99|^8|^9|dev-master",
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "^5.7",
        "mikey179/vfsStream": "* ",
        "satooshi/php-coveralls": "*",
        "namelesscoder/typo3-repository-client": "*"
    },
    "bin": [
        "bin/generateschema-typo3",
        "bin/stage",
        "bin/release",
        "bin/changelog"
    ]
}
